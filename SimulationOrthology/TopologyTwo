	tree tree_1 = [&R] (((taxon_1:1.0,(taxon_3:1.0,taxon_2:1.0):1.0):1.0,(taxon_4:1.0,taxon_5:1.0):1.0):1.0,taxon_6:1.0);
	tree tree_2 = [&R] (((taxon_1:1.0,(taxon_4:1.0,taxon_5:1.0):1.0):1.0,(taxon_3:1.0,taxon_2:1.0):1.0):1.0,taxon_6:1.0);
